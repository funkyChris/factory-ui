<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.0.0-rc1//EN" "http://google-web-toolkit.googlecode.com/svn/tags/2.0.0-rc1/distro-source/core/src/gwt-module.dtd">
	<!--
		Authors Jerome Blanchard <jayblanc@gmail.com> - Christophe Bouthier <christophe.bouthier@loria.fr>
		Date 4 December 2009
	-->
<module rename-to='core'>
	<inherits name='com.google.gwt.user.User' />
	<inherits name='com.google.gwt.user.theme.standard.Standard' />
	<inherits name='com.google.gwt.uibinder.UiBinder' />
    <inherits name='org.qualipso.factory.ui.core.utils.Utils' />

	<source path='client' />
    
	<entry-point class='org.qualipso.factory.ui.core.core.client.Core' />
    
     <set-property name="user.agent" value="gecko" />
     
    <define-linker name="component" class="org.qualipso.factory.ui.core.utils.linker.ComponentLinker" />
    <add-linker name="component" />
</module>
